{
    "name": "friendsofsymfony/psr-http-bundle",
    "type": "symfony-bundle",
    "description": "A bundle that enables applications to use PSR-7 message objects in Symfony controllers",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander M. Turek",
            "email": "me@derrabus.de"
        },
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        }
    ],
    "require": {
        "php": ">= 7.2.5",
        "nyholm/psr7": "^1.3",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "symfony/config": "^4.4 || ^5.2",
        "symfony/dependency-injection": "^4.4 || ^5.2",
        "symfony/event-dispatcher": "^4.4 || ^5.2",
        "symfony/http-foundation": "^4.4 || ^5.2",
        "symfony/http-kernel": "^4.4 || ^5.2",
        "symfony/psr-http-message-bridge": "^1.1 || ^2"
    },
    "require-dev": {
        "symfony/browser-kit": "^4.4 || ^5.2",
        "symfony/framework-bundle": "^4.4 || ^5.2",
        "symfony/phpunit-bridge": "^5.2"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "FOS\\PsrHttpBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FOS\\PsrHttpBundle\\Tests\\": "tests"
        }
    }
}
